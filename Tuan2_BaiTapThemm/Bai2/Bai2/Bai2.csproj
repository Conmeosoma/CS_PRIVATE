<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>disable</ImplicitUsings>        <!-- Tắt usings tự động -->
        <Nullable>disable</Nullable>        <!-- Tùy chọn, nếu không thích warning nullable -->
        <UseTopLevelStatements>false</UseTopLevelStatements>        <!-- QUAN TRỌNG: tắt top-level -->
    </PropertyGroup>

</Project>
